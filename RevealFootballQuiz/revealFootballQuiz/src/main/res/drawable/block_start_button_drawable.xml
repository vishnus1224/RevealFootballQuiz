<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" android:drawable="@drawable/block_start_button_click">
        <shape android:shape="rectangle"  >
         <corners android:radius="3dip" />
         <stroke android:width="5dip" android:color="#5e7974" />
         </shape>
    </item>
    <item android:state_pressed="false" android:drawable="@drawable/block_start_button">
        <shape android:shape="rectangle"  >
         <corners android:radius="3dip" />
         <stroke android:width="5dip" android:color="#5e7974" />
     </shape>
    </item>

</selector>